   <configIni use="">
   </configIni>

   <launcherArgs>
      <programArgs>-product &quot;com,ma&quot;
      </programArgs>
      <vmArgs>-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=8272
      </vmArgs>
   </launcherArgs>

   <windowImages/>

